; Engine configuration file.
; It's best edited using the Godot editor.
; If edited manually, be sure to keep the structure.

[application]

config/name="UltimateShooter"
run/main_scene="res://scenes/Lobby.tscn"
config/icon="res://icon.png"

[display]

window/size/viewport_width = 1280
window/size/viewport_height = 720
window/stretch/mode = "viewport"
window/stretch/aspect = "keep"
window/vsync/use_vsync = true

[rendering]

renderer/rendering_method = "forward_plus"
quality/driver/driver_name = "GLES3"

[input]

ui_left = [ { "deadzone": 0.5, "event": { "type": "joy_axis", "axis": 0, "device": -1, "axis_value": -1.0 } } ]
ui_right = [ { "deadzone": 0.5, "event": { "type": "joy_axis", "axis": 0, "device": -1, "axis_value": 1.0 } } ]
ui_up = [ { "deadzone": 0.5, "event": { "type": "joy_axis", "axis": 1, "device": -1, "axis_value": -1.0 } } ]
ui_down = [ { "deadzone": 0.5, "event": { "type": "joy_axis", "axis": 1, "device": -1, "axis_value": 1.0 } } ]

[rendering/environment/default_environment]

environment = "res://default_env.tres"

